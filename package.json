{
  "name": "trakt-to-letterboxd",
  "version": "1.0.1",
  "description": "Extract trakt movie history and export to csv for uploading to letterboxd",
  "main": "dist/main/index.js",
  "repository": "git@github.com:bbeesley/trakt-to-letterboxd.git",
  "author": "Bill Beesley <bbeesley@me.com>",
  "license": "MIT",
  "bin": {
    "trakt-to-letterboxd": "./dist/ttl.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "BABEL_ENV=production babel src --out-dir dist --copy-files --source-maps --extensions \".js\" --ignore \"src/test/**\" && chmod +x dist/ttl.js",
    "postcompile": "./node_modules/.bin/documentation readme src/main/index.js -s \"API\" --github",
    "lint": "./node_modules/.bin/eslint src/**  --fix",
    "mocha": "BABEL_ENV=test nyc mocha --require ./src/test/index.js 'src/test/**/*.test.js'",
    "flow": "flow",
    "flow:delete": "find ./dist/main -name *.flow -delete || :",
    "flow:generate": "flow-copy-source -v ./src/main ./dist/main",
    "pretest": "yarn flow:delete && run-p clean lint flow",
    "posttest": "yarn compile && yarn flow:generate",
    "test": "BABEL_ENV=test yarn mocha"
  },
  "devDependencies": {
    "arrow-mocha": "^1.0.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-rewire": "^1.1.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-template": "^6.26.0",
    "babel-types": "^6.26.0",
    "chai": "^4.1.2",
    "documentation": "^12.1.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "fetch-mock": "^6.3.0",
    "flow-bin": "^0.69.0",
    "flow-copy-source": "^1.3.0",
    "ignore-styles": "^5.0.1",
    "mocha": "^5.0.5",
    "mocha-sinon": "^2.0.0",
    "nodemon": "^1.17.3",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.6.0",
    "sinon": "^4.5.0",
    "sinon-chai": "^3.0.0"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "csv-builder": "^1.0.0",
    "fecha": "^2.3.3",
    "node-fetch": "^2.1.2",
    "yargs": "^11.0.0"
  }
}
